{
  "_id": "admin.instance",
  "_type": "page.admin",
  "url": "/instance/:_id",
  "heading": "{instanceTitle}",
  "components": [
    {
      "_id": "admin.instance--header",
      "_type": "group",
      "components": [
        {
          "_id": "admin.instance--id",
          "_type": "content",
          "html": "Id {_id}"
        },
        {
          "_id": "admin.instance--type",
          "_type": "content",
          "html": "{instanceCategory, select, undefined{Component} other{{instanceCategory}}} type {instanceType}",
          "show": {
            "identifier": "instanceType",
            "operator": "defined"
          }
        },
        {
          "_id": "admin.instance--used",
          "_type": "content",
          "html":
            "Used by {usedBy}{usedWhere, select, undefined{} other{ in {usedWhere}}}",
          "show": {
            "identifier": "usedBy",
            "operator": "defined"
          }
        },
        {
          "_id": "admin.instance--navigation",
          "_type": "content",
          "html": "{navigationPrevious}\n\n{navigationNext}",
          "show": {
            "identifier": "navigationNext",
            "operator": "defined"
          }
        },
        {
          "_id": "admin.instance--url",
          "_type": "content",
          "html":
            "- URL [{instanceUrl}]({instanceUrl})\n- [Edit mode]({instanceUrl}/edit)\n- [Preview mode]({instanceUrl}/preview)",
          "show": {
            "identifier": "instanceUrl",
            "operator": "defined"
          }
        }
      ]
    },
    {
      "_id": "admin.instance--properties.main",
      "_type": "propertylist",
      "items": []
    },
    {
      "_id": "admin.instance--properties.repeatable",
      "_type": "details",
      "summary": "Repeatable properties",
      "show": {
        "identifier": "repeatableProperties",
        "operator": "defined"
      }
    },
    {
      "_id": "admin.instance--properties.validation",
      "_type": "details",
      "summary": "Validation and error messages",
      "show": {
        "identifier": "validationProperties",
        "operator": "defined"
      }
    },
    {
      "_id": "admin.instance--properties.errors",
      "_type": "details",
      "summary": "Other error messages",
      "show": {
        "identifier": "errorsProperties",
        "operator": "defined"
      }
    },
    {
      "_id": "admin.instance--properties.logic",
      "_type": "details",
      "summary": "Logical properties",
      "show": {
        "identifier": "logicProperties",
        "operator": "defined"
      }
    },
    {
      "_id": "admin.instance--properties.content",
      "_type": "details",
      "summary": "Content properties",
      "show": {
        "identifier": "contentProperties",
        "operator": "defined"
      }
    },
    {
      "_id": "admin.instance--properties.html",
      "_type": "details",
      "summary": "HTML properties",
      "show": {
        "identifier": "htmlProperties",
        "operator": "defined"
      }
    },
    {
      "_id": "admin.instance--properties.additional",
      "_type": "details",
      "summary": "Additional properties",
      "show": {
        "identifier": "additionalProperties",
        "operator": "defined"
      }
    },
    {
      "_id": "admin.instance--instance",
      "_type": "details",
      "summary": "View raw instance data",
      "components": [
        {
          "_id": "admin.instance--instance--form",
          "_type": "form",
          "components": [
            {
              "_id": "admin.instance--instance--data",
              "name": "instance",
              "_type": "textarea",
              "label": "Instance",
              "errors": {
                "instance.invalid": {
                  "inline": "JSON is invalid",
                  "summary": "‘{control}’'s JSON is invalid"
                },
                "instance.parse": {
                  "inline": "JSON cannot be parsed",
                  "summary": "‘{control}’'s JSON cannot be parsed"
                }
              }
            },
            {
              "_id": "admin.instance--instance--button",
              "_type": "button",
              "html": "Save instance"
            }
          ]
        },
        {
          "_id": "admin.instance--expanded.instance",
          "_type": "details",
          "summary": "View expanded instance",
          "components": [
            {
              "_id": "admin.instance--expanded.instance--content",
              "_type": "content",
              "html": "```\n{expandedInstance}\n```"
            }
          ],
          "show": {
            "identifier": "expandedInstance",
            "operator": "defined"
          }
        }
      ]
    },
    {
      "_id": "admin.instance--instance--form--delete",
      "_type": "form",
      "method": "post",
      "components": [
        {
          "_id": "admin.instance--instance--delete",
          "_type": "button",
          "name": "delete-instance",
          "value": "yes",
          "html": "Delete instance",
          "classes": "fb-action--delete",
          "show": {
            "identifier": "deletable",
            "operator": "defined"
          }
        }
      ]
    }
  ]
}
